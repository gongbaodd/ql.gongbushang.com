  "version": "0.1.0",
  "description": "A Strapi application",
  "devDependencies": {
    "audit-ci": "^2.5.1",
    "commitizen": "^4.0.3",
    "emoji-cz": "^0.3.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "gh-pages": "^2.2.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "prettier": "^1.19.1"
  },
  "dependencies": {
    "strapi": "3.0.0-beta.19.3",
    "strapi-admin": "3.0.0-beta.19.3",
    "strapi-utils": "3.0.0-beta.19.3",
    "strapi-plugin-content-type-builder": "3.0.0-beta.19.3",
    "strapi-plugin-content-manager": "3.0.0-beta.19.3",
    "strapi-plugin-users-permissions": "3.0.0-beta.19.3",
    "strapi-plugin-email": "3.0.0-beta.19.3",
    "strapi-plugin-upload": "3.0.0-beta.19.3",
    "strapi-connector-bookshelf": "3.0.0-beta.19.3",
    "knex": "<0.20.0",
    "sqlite3": "latest"
  },
  "author": {
    "name": "A Strapi developer"
  },
  "strapi": {
    "uuid": "57c0279d-d352-428c-b5a5-0de1a3732a3e"
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "yarn audit:ci && lint-staged"
    }
  },
  "lint-staged": {
    "*.js(x)|*.json|*.ts(x)|*.markdown|*.md": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "emoji-cz"
    }
  },
  "scripts": {
    "develop": "strapi develop",
    "start": "strapi start",
    "build": "strapi build",
    "deploy": "sh scripts/docker_push.sh",
    "strapi": "strapi",
    "audit:ci": "audit-ci -m -a 1065 -a 1469",
    "commit": "git-cz",
    "test": "echo 'no test'"
  }
}
